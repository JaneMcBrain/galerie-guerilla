.container {
    padding-left: 10%;
    padding-right: 10%;
    padding-top: 10%;
    padding-bottom: 10%;
    left: auto;
    top: auto;
    right: auto;
    bottom: auto;
    background-color: rgb(233, 233, 233);
    position: relative;
    height: 100%;
}

.button {
    font-size: 32px;
    color: rgb(55, 55, 55);
    -unity-background-image-tint-color: rgb(217, 217, 217);
    background-color: rgb(255, 255, 255);
    padding-top: 20px;
    padding-bottom: 20px;
    border-left-color: rgb(255, 247, 247);
    border-right-color: rgb(255, 247, 247);
    border-top-color: rgb(255, 247, 247);
    border-bottom-color: rgb(255, 247, 247);
    border-left-width: 10px;
    border-right-width: 10px;
    border-top-width: 10px;
    border-bottom-width: 10px;
    -unity-font-style: bold;
    padding-left: 20px;
    padding-right: 20px;
    -unity-background-scale-mode: scale-to-fit;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    flex-basis: auto;
}

.button__icon {
    position: absolute;
    right: 20px;
    bottom: 20px;
    width: 40px;
    height: 40px;
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 20px;
    padding-bottom: 20px;
}

.icon-button {
    height: 100%;
    width: 20%;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgba(188, 188, 188, 0);
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 20px;
    padding-bottom: 20px;
}

.icon-button__icon {
    width: 100%;
    height: 100%;
    -unity-background-scale-mode: scale-to-fit;
}

.icon-button__icon--info {
    background-image: url('project://database/Assets/Icons/info.png?fileID=2800000&guid=6786ab31598084715a9c71f92cd36e88&type=3#info');
    -unity-background-scale-mode: scale-to-fit;
}

.icon-button__icon--info:hover {
    background-image: url('project://database/Assets/Icons/info-primary.png?fileID=2800000&guid=c92969cb30f994b3f96f89f7735c74f7&type=3#info-primary');
}

.icon-button__icon--info.is-active {
    background-image: url('project://database/Assets/Icons/info-primary.png?fileID=2800000&guid=c92969cb30f994b3f96f89f7735c74f7&type=3#info-primary');
}

.icon-button__icon--account {
    background-image: url('project://database/Assets/Icons/user.png?fileID=2800000&guid=9e4236e8d0734447eaa86f320b926b8c&type=3#user');
}

