.icon-button__icon--location:hover {
    background-image: url('project://database/Assets/Icons/locations-primary.png?fileID=2800000&guid=d5f5f478b7f0a472c99bb09e6b25633e&type=3#locations-primary');
}

.icon-button__icon--location.is-active {
    background-image: url('project://database/Assets/Icons/locations-primary.png?fileID=2800000&guid=d5f5f478b7f0a472c99bb09e6b25633e&type=3#locations-primary');
}

.icon-button__icon--tour {
    background-image: url('project://database/Assets/Icons/flag.png?fileID=2800000&guid=fef09fa05a89142a099b269476e4db56&type=3#flag');
}

.icon-button__icon--tour:hover {
    background-image: url('project://database/Assets/Icons/flag-primary.png?fileID=2800000&guid=62adec27ecf1a4c9980a9d22820075b2&type=3#flag-primary');
}

.icon-button__icon--tour.is-active {
    background-image: url('project://database/Assets/Icons/flag-primary.png?fileID=2800000&guid=62adec27ecf1a4c9980a9d22820075b2&type=3#flag-primary');
}

.input {
    -unity-background-image-tint-color: rgb(194, 223, 255);
}

.card-list {
    margin-top: 32px;
    height: 100%;
}

.card-list-item {
    flex-direction: row;
    height: 100px;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: flex-start;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    background-color: rgb(255, 255, 255);
    margin-bottom: 8px;
}

.card-list-item:hover {
    background-color: rgb(221, 221, 221);
}

.h1 {
    font-size: 100%;
}
